<Window x:Class="Konstantinova_eyes_save.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Konstantinova_eyes_save"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="800" MinWidth="1300" WindowStartupLocation="CenterScreen" Icon="/res/Глазки-save.png" FontFamily="Century Gothic">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FFE9F9"></Grid>
        <Grid Grid.Row="2" Background="#FFE9F9"></Grid>
        
        <Image Source="/res/Глазки-save.png" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="5"></Image>
        <TextBlock Text="Глазки-save" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>

        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack" Click="BtnBack_Click" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="5"></Button>
        
    </Grid>
</Window>
